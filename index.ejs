<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Property Management System</title>
</head>
<body>
    <h1>Welcome to Property Management System</h1>
    <button onclick="showProperties()">Show Properties</button>
    <div id="propertyList"></div>
    
    <script>
        function showProperties() {
            fetch('/properties')
            .then(response => response.json())
            .then(data => {
                let propertyList = document.getElementById('propertyList');
                propertyList.innerHTML = '';
                data.forEach(property => {
                    propertyList.innerHTML += `<p>${property.PropertyId}: $${property.Price}</p>`;
                });
            });
        }
    </script>
</body>
</html>
